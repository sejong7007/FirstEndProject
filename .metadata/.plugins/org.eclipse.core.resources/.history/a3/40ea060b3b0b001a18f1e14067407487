package com.testtwo.web.pxy;

import org.springframework.context.annotation.Lazy;
import org.springframework.stereotype.Component;

import lombok.Data;

@Data
@Lazy
@Component("pager")
public class PageProxy extends Proxy {

	private int rowCount, rowNum, startRow, endRow, 
				pageSize, pageNum, pageCount, startPage, endPage,
				blockSize, blockNum, blockCount, startBlock, endBlock;
	
	
}
